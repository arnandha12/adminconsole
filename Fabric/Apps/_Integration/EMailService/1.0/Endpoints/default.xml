<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<endpoint name="default" type="JSON" url="https://_$_AC_KMS_URL_$_" encryptSecureInfo="false">
    <config>
        <entry>
            <key>ignorehttpproxy</key>
            <value>true</value>
        </entry>
        <entry>
            <key>urlprovider</key>
            <value>com.kony.adminconsole.commons.handler.ServiceURLProvider</value>
        </entry>
    </config>
    <apiThrottling>
        <entry>
            <key>rateLimitPerIP</key>
            <value>10</value>
        </entry>
        <entry>
            <key>totalRateLimit</key>
            <value>100</value>
        </entry>
    </apiThrottling>
    <jsonpath>true</jsonpath>
</endpoint>
